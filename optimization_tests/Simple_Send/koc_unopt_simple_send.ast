broadcast_opt
  freq :=
  if
    A.(freq > 0)
  then
    A[L] ~> B;
    A[L] ~> D;
    A[L] ~> E;
    A[L] ~> F;
    A[L] ~> G;
    A[L] ~> C;
    let
      C.x := C.(10);
    in
      let
        D.x := D.(10);
      in
        let
          E.x := E.(10);
        in
          let
            F.x := F.(10);
          in
            let
              G.x := G.(10);
            in
              broadcast_opt A.(freq - 1)
  else
    A[R] ~> B;
    A[R] ~> C;
    A[R] ~> D;
    A[R] ~> E;
    A[R] ~> F;
    A[R] ~> G;
    let
      B.x := B.(9);
    in
      let
        C.x := C.(9);
      in
        let
          D.x := D.(9);
        in
          let
            E.x := E.(9);
          in
            let
              F.x := F.(9);
            in
              let
                G.x := G.(9);
              in
                A.(print_endline) A.("Terminate - Unoptimized");
main := broadcast_opt A.(1000);
